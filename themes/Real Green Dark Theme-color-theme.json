			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#8ee4af"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#8ee4af"
			}
